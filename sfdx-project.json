{
    "packageDirectories": [
        {
            "path": "./src-env-specific-pre",
            "package": "src-env-specific-pre",
            "versionNumber": "1.0.0.0",
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "./src-env-specific-alias-pre",
            "package": "src-env-specific-alias-pre",
            "versionNumber": "1.0.0.0",
            "aliasfy": true,
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "src/frameworks/trigger-framework",
            "package": "trigger-framework",
            "versionNumber": "1.0.0.0",
            "versionName": "trigger-framework-1.0"
        },
        {
            "path": "src/frameworks/trigger-bypass-strategy",
            "package": "trigger-bypass-strategy",
            "versionNumber": "1.0.0.0",
            "versionName": "trigger-bypass-strategy-1.0",
            "dependencies": [
                {
                    "package": "trigger-framework",
                    "versionNumber": "1.0.0.LATEST"
                }
            ]
        },
        {
            "path": "./src/core-crm",
            "package": "core",
            "versionName": "Version 1.0.0.0",
            "versionNumber": "1.0.0.0",
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "./src-ui",
            "package": "src-ui",
            "versionNumber": "1.1.0.0",
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "./src-access-management",
            "package": "src-access-management",
            "versionNumber": "1.0.0.0",
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "./src-env-specific-alias-post",
            "package": "src-env-specific-alias-post",
            "versionNumber": "1.0.0.0",
            "aliasfy": true,
            "alwaysDeploy": true,
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        },
        {
            "path": "./src-temp",
            "package": "src-temp",
            "versionName": "Version 1.0.0",
            "versionNumber": "1.0.0.0",
            "default": true,
            "ignoreOnStage": [
                "prepare",
                "validate",
                "quickbuild",
                "build"
            ]
        }
    ],
    "namespace": "",
    "sfdcLoginUrl": "https://login.salesforce.com",
    "sourceApiVersion": "52.0",
    "packageAliases": {
    },
    "plugins": {
        "sfpowerscripts": {
            "ignoreFiles": {
                "quickbuild": "forceignores/.buildignore",
                "build": "forceignores/.buildignore"
            }
        }
    }
}